  <div class="field"  style="color: black; border-radius: 4px; background-color: #90A4AE; width: 800px;   position:absolute; border: solid black 1px; top:50%; left:25%; margin-left: -100px; margin-top:-200px;" align="center">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
<br>
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  <% if devise_mapping.rememberable? -%>
   <br>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
  <br>
  <% end %> 
    <%= f.submit "Log in" %>

<% end %>
